host = "localhost"
port = 9000
requestCacheSize = 10
logfile = "/var/log/deb-drop/deb-drop.log"
repoLocation = "/ftp/pool"
tmpDir = "/tmp/deb-drop"
repoRebuildCommand = "regenerate_repo.sh -r"

[[token]]
value = "someToken"
owner = "somebody"
[[token.repo]]
name = "someRepo-stable-amd64"
[[token.repo]]
name = "someRepo-jessie-amd64"

[[token]]
value = "anotherToken"
owner = "someoneElse"
[[token.repo]]
name = "someRepo-stable-amd64"
[[token.repo]]
name = "someRepo-jessie-amd64"
